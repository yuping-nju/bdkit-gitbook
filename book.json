{
    "author": "Copyright ICS@NJU",
    "description": "BDKit GitBook",
    "extension": null,
    "generator": "site",
    "isbn": "",
    "links": {
        "sidebar": {
            "BDKit Homepage": "http://bdkit.cn:30080"
        }
    },
    "output": null,
    "pdf": {
        "fontSize": 12,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin": {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": false,
        "paperSize": "a4"
    },
    "plugins": [
        "expandable-chapters",
        "theme-comscore",
        "lightbox",
        "-sharing", "sharing-plus",
        "anchor-navigation-ex",
        "tbfed-pagefooter",
        "code",
        "hide-element"
    ],
    "pluginsConfig": {
        "sharing": {
           "douban": false,
           "facebook": false,
           "google": false,
           "hatenaBookmark": false,
           "instapaper": false,
           "line": false,
           "linkedin": false,
           "messenger": false,
           "pocket": false,
           "qq": false,
           "qzone": true,
           "stumbleupon": false,
           "twitter": false,
           "viber": false,
           "vk": false,
           "weibo": true,
           "whatsapp": false,
           "all": [
               "facebook", "google", "twitter",
               "weibo"
           ]
       },
       "anchor-navigation-ex": {
            "showLevel": false,
            "associatedWithSummary": true,
            "printLog": false,
            "multipleH1": true,
            "mode": "float",
            "showGoTop":true,
            "float": {
                "floatIcon": "fa fa-navicon",
               "showLevelIcon": true,
               "level1Icon": "fa fa-hand-o-right",
              "level2Icon": "fa fa-hand-o-right",
              "level3Icon": "fa fa-hand-o-right"
            },
            "pageTop": {
               "showLevelIcon": true,
             "level1Icon": "fa fa-hand-o-right",
             "level2Icon": "fa fa-hand-o-right",
            "level3Icon": "fa fa-hand-o-right"
            }
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright ICS@NJU",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "code": {
            "copyButtons": true
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        }
    },
    "title": "BDKit GitBook",
    "variables": {}
}